<!--List<Employee>-->
@model List<Employee>
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<table class="table table-bordered table-hover">
    <tr>
        <th>ID</th>
        <th>Name</th>
        <th>PRofile Image</th>
    </tr>
    @foreach (Employee item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.ImageURL</td>
            <td>
                <img src="/Images/@item.ImageURL" style="width:100px"/>
            </td>
        </tr>
    }
</table>

